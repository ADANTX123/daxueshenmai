{
  "manifest_version": 3,
  "name": "祝你大雪深埋",
  "version": "1.0",
  "description": "按学科评估或省份展示高校入口，助你一键访问！",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["tabs", "storage", "scripting"],
  "host_permissions": [
    "*://*/*"
  ],
  "icons": {
    "128": "icon.png"
  }
}
